{
  "name": "hidetaka-dev",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "cf:build": "npx opennextjs-cloudflare build",
    "cf:preview": "npm run cf:build && npx wrangler dev",
    "cf:dev": "npm run cf:build && npx opennextjs-cloudflare preview",
    "cf:deploy": "npm run cf:build && npx opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@microsoft/clarity": "^1.0.0",
    "@opennextjs/cloudflare": "^1.12.0",
    "dayjs": "^1.11.7",
    "fast-xml-parser": "^4.2.4",
    "microcms-js-sdk": "^2.3.2",
    "next": "^16.0.7",
    "next-google-ads": "^1.2.0",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5",
    "vitest": "^4.0.9",
    "wrangler": "^4.38.0"
  }
}
